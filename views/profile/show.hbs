{{#if translator}}
<h1>{{translator.user.username}}</h1>

{{#if translator.profileImageUrl}}
<div class="profile-img-wrapper">
    <img class="profile-img" src="{{translator.profileImageUrl}}" alt="profile Image">
</div>
{{/if}}

<div class="card-wrapper">
    <div class="card-wrapper-headline">
        <span>General Information</span>
    </div>
    <div class="info-text">
        <p>Role: {{translator.role}}</p>

        <p>Rating: {{translator.rating}}</p>
    </div>

</div>

{{#if isProfessional}}
<p>Fee: â‚¬ {{translator.price}} /hr </p>
{{/if}}





<p>E-Mail: {{translator.user.email}}</p>
<p>Phone Number: {{translator.telephone}}</p>
<p>Location: {{translator.location}}</p>

<div class="language-wrapper">
    <p>Languages</p>
    <ul class="languages-list">
        {{#each translator.languages}}
        <li>{{this.language}} Level: {{this.level}}</li>
        {{/each}}
    </ul>
</div>

<div class="background-wrapper">
    <p>Background</p>
    <ul class="background-list">
        {{#each translator.background}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
</div>

<div class="setting-wrapper">
    <p>Preferred Setting</p>
    <ul class="setting-list">
        {{#each translator.preferedSetting}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
</div>

<a href="/profile/edit">Edit Profile</a>

{{/if}} {{#if wo}}
<h1>Youth Welfare Officer Profile</h1>
{{#if wo.profileImageUrl}}
<div class="profile-img">
    <img src="{{wo.profileImageUrl}}" alt="profile Image">
</div>
{{/if}}

<p>Username: {{wo.user.username}}</p>
<p>Role: Youth Welfare Officer</p>
<p>E-Mail: {{wo.user.email}}</p>
<p>Location: {{wo.location}}</p>
{{/if}}